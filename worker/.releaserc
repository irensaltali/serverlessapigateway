{
    "branches": [
        "main", "31-add-relaese-automation-and-versioning"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "assets": [
            {"path": "src/*"},
            {"path": "*.ts"},
            {"path": "package*"},
            {"path": "wrangler.toml"}
          ],
          "addReleases": "bottom",
          "successComment": false
        }
      ]
    ]
}
